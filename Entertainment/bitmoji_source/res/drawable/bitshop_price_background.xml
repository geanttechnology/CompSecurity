<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/bitshop_price_background" />
            <stroke android:width="1.0dip" android:color="@color/imoji_primary" />
            <corners android:radius="@dimen/bitshop_price_corner_radius" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/bitshop_price_background" />
            <stroke android:width="1.0dip" android:color="@color/bitshop_price" />
            <corners android:radius="@dimen/bitshop_price_corner_radius" />
        </shape>
    </item>
</selector>