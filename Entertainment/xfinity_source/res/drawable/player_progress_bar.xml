    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#ff2c98c9" android:endColor="#ff2c98c9" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff2c98c9" />
