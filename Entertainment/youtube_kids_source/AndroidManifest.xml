            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE"/>
            </intent-filter>
        </receiver>
        <provider android:authorities="com.google.android.apps.youtube.kids.SuggestionProvider" android:exported="false" android:name="com.google.android.apps.youtube.kids.suggest.KidsSuggestionProvider"/>
    </application>
</manifest>
