        <activity android:configChanges="keyboardHidden|orientation" android:excludeFromRecents="true" android:name="com.google.userfeedback.android.api.PreviewActivity" android:screenOrientation="sensorLandscape"/>
        <activity android:configChanges="keyboardHidden|orientation" android:excludeFromRecents="true" android:name="com.google.userfeedback.android.api.ShowTextActivity" android:screenOrientation="sensorLandscape"/>
        <activity android:configChanges="keyboardHidden|orientation" android:excludeFromRecents="true" android:name="com.google.userfeedback.android.api.ShowStringListActivity" android:screenOrientation="sensorLandscape"/>
