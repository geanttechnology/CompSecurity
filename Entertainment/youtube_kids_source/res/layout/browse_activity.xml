                    <com.google.android.apps.youtube.kids.ui.YouTubeKidsTextView android:textSize="@dimen/tab_title_text_size" android:textStyle="normal" android:textColor="#ffe62117" android:layout_gravity="top|center" android:id="@id/tab_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textIsSelectable="false" ytkids:capitalize="false" ytkids:roboto2Font="medium" />
