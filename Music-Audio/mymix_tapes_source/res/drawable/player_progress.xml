    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#ffcecfce" android:endColor="#ffcecfce" />
            <size android:height="2.0dip" android:width="20.0dip" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <size android:height="2.0dip" android:width="20.0dip" />
                <gradient android:startColor="#96b52525" android:endColor="#96b52525" android:centerColor="#96b52525" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <size android:height="2.0dip" android:width="20.0dip" />
                <gradient android:startColor="#ffb52525" android:endColor="#ffb52525" android:centerColor="#ffb52525" />
            </shape>
        </clip>
    </item>
</layer-list>