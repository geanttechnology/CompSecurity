<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/bc_wechat_main_color_pressed" />
            <stroke android:width="0.0dip" android:color="@color/bc_wechat_sub_color" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/bc_wechat_main_color" />
            <stroke android:width="0.0dip" android:color="@color/bc_wechat_sub_color" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
</selector>