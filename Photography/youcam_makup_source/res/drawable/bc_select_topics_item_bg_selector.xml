<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:left="20.0px" android:top="20.0px" android:right="20.0px" android:bottom="20.0px">
        <selector>
            <item android:state_pressed="true">
                <shape android:shape="oval">
                    <solid android:color="#fff3dcdb" />
                    <stroke android:width="2.0dip" android:color="#ffc3a4cf" />
                </shape>
            </item>
            <item android:state_selected="true">
                <shape android:shape="oval">
                    <solid android:color="#fff3dcdb" />
                    <stroke android:width="2.0dip" android:color="#fff05984" />
                </shape>
            </item>
            <item>
                <shape android:shape="oval">
                    <solid android:color="#ffffffff" />
                    <stroke android:width="2.0dip" android:color="#ffc3a4cf" />
                </shape>
            </item>
        </selector>
    </item>
</layer-list>