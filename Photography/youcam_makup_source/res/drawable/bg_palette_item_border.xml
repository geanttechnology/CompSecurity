<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
    <item android:state_selected="true" android:left="0.0dip" android:top="0.0dip" android:right="0.0dip" android:bottom="0.0dip">
        <shape android:shape="rectangle">
            <solid android:color="#ffb27ff7" />
        </shape>
    </item>
    <item android:state_pressed="true" android:left="0.0dip" android:top="0.0dip" android:right="0.0dip" android:bottom="0.0dip">
        <shape android:shape="rectangle">
            <solid android:color="#ff7c59ac" />
        </shape>
    </item>
</selector>