<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <layer-list>
            <item>
                <shape>
                    <stroke android:width="1.0dip" android:color="#b2000000" />
                    <solid android:color="#ccffffff" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <stroke android:width="2.0dip" android:color="@android:color/transparent" />
                <solid android:color="#ff9789ba" />
            </shape>
        </clip>
    </item>
</layer-list>