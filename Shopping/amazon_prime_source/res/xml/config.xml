<?xml version="1.0" encoding="utf-8"?>
<widget id="io.cordova.helloCordova" version="2.0.0" xmlns="http://www.w3.org/ns/widgets">
    <access origin="*" />
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="MASHCore">
        <param name="android-package" value="com.amazon.now.mash.api.MASHHoudiniCorePlugin" />
    </feature>
    <feature name="MASHEvent">
        <param name="android-package" value="com.amazon.mobile.mash.api.MASHEventPlugin" />
    </feature>
    <feature name="MASHMShopAuthentication">
        <param name="android-package" value="com.amazon.now.mash.api.MASHMShopAuthenticationPlugin" />
    </feature>
    <feature name="MASHNavigation">
        <param name="android-package" value="com.amazon.now.mash.api.MASHNavigationPlugin" />
    </feature>
    <feature name="MASHCart">
        <param name="android-package" value="com.amazon.now.mash.api.MASHCartPlugin" />
    </feature>
    <feature name="Houdini">
        <param name="android-package" value="com.amazon.now.mash.api.MASHHoudiniPlugin" />
    </feature>
    <feature name="MASHImageGallery">
        <param name="android-package" value="com.amazon.now.mash.api.MASHImageGalleryPlugin" />
