<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ff4d4d4d" android:endColor="#ff808080" android:angle="270.0" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#fffaae17" android:endColor="#ffec7423" android:angle="270.0" android:centerY="0.5" android:centerColor="#fff7971d" />
            </shape>
        </clip>
    </item>
</layer-list>