<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_window_focused="false" android:state_enabled="true" android:drawable="@drawable/btn_state_common_search_normal" />
    <item android:state_window_focused="false" android:state_enabled="false" android:drawable="@drawable/btn_state_common_search_disabled" />
    <item android:state_pressed="true" android:drawable="@drawable/btn_state_common_search_pressed" />
    <item android:state_focused="true" android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/style_guide_canvas" />
            <corners android:radius="@dimen/buttonCornerRadius" />
        </shape>
    </item>
    <item android:state_enabled="true" android:drawable="@drawable/btn_state_common_search_normal" />
    <item android:state_focused="true" android:drawable="@drawable/btn_state_common_search_disabled_focused" />
    <item android:drawable="@drawable/btn_state_common_search_disabled" />
</selector>