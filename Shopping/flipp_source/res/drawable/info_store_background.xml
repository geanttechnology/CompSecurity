<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/infoCellAccentColor" />
            <stroke android:width="1.0dip" android:color="@color/infoCellBorderColor" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/infoCellAccentColor" />
            <stroke android:width="1.0dip" android:color="@color/infoCellBorderColor" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/infoCellBackgroundColor" />
            <stroke android:width="1.0dip" android:color="@color/infoCellBorderColor" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
</selector>