    <style name="AddPhotoButtonStyle" parent="@style/DefaultAddButtonStyle">
        <item name="android:borderTop">true</item>
    </style>
    <style name="Base.TextAppearance.AppCompat.Button" parent="@style/Base.TextAppearance.AppCompat">
        <item name="android:textSize">@dimen/abc_text_size_button_material</item>
        <item name="android:textColor">?android:textColorPrimary</item>
        <item name="android:borderTop">true</item>
    </style>
    <style name="ContactButtonStyle" parent="@style/DefaultContactButtonStyle">
        <item name="android:borderTop">false</item>
    </style>
    <style name="DefaultSignUpButtonStyleCommon.Custom" parent="@style/DefaultSignUpButtonStyleCommon">
        <item name="android:borderTop">false</item>
    </style>
    <style name="EditProfileButtonStyle">
        <item name="android:textColor">@color/common_color_dark</item>
        <item name="android:gravity">left|center</item>
        <item name="android:background">@drawable/background_elipse_user</item>
        <item name="android:padding">@dimen/padding_large</item>
