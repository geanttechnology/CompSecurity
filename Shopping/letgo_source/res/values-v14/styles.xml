    <style name="TextAppearance.StatusBar.EventContent.Line2" parent="@style/TextAppearance.StatusBar.EventContent">
        <item name="android:textSize">@dimen/notification_subtext_size</item>
    </style>
    <style name="TextAppearance.StatusBar.EventContent.Time" parent="@style/TextAppearance.StatusBar.EventContent" />
    <style name="TextAppearance.StatusBar.EventContent.Title" parent="@android:style/TextAppearance.StatusBar.EventContent.Title" />
    <style name="button_pink">
        <item name="android:textSize">@dimen/font_size_regular</item>
        <item name="android:textColor">@color/text_color_white</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/button_pink</item>
        <item name="android:paddingLeft">20.0dip</item>
        <item name="android:paddingRight">20.0dip</item>
        <item name="android:layout_marginTop">15.0dip</item>
        <item name="android:layout_marginBottom">15.0dip</item>
        <item name="android:minWidth">200.0dip</item>
        <item name="android:minHeight">44.0dip</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:borderTop">false</item>
    </style>
    <style name="Platform.V14.AppCompat" parent="@style/Platform.V11.AppCompat">
        <item name="android:searchViewGoIcon">?actionModeSelectAllDrawable</item>
        <item name="android:thumbTextPadding">@dimen/abc_list_item_padding_horizontal_material</item>
        <item name="android:switchTextAppearance">@dimen/abc_list_item_padding_horizontal_material</item>
    </style>
    <style name="Platform.V14.AppCompat.Light" parent="@style/Platform.V11.AppCompat.Light">
        <item name="android:searchViewGoIcon">?actionModeSelectAllDrawable</item>
        <item name="android:thumbTextPadding">@dimen/abc_list_item_padding_horizontal_material</item>
        <item name="android:switchTextAppearance">@dimen/abc_list_item_padding_horizontal_material</item>
    </style>
